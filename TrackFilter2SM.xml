				</ProxyGroupDomain>
				<DataTypeDomain name="input_type">
					<DataType value="vtkPolyData"/>
				</DataTypeDomain>
				<InputArrayDomain name="input_array">
					<RequiredProperties>
						<Property name="SelectInputArray" 
							function="FieldDataSelection"/>
						</RequiredProperties>
				</InputArrayDomain>
			</InputProperty>
			
			
		</SourceProxy>
	</ProxyGroup>
</ServerManagerConfiguration>

