<ServerManagerConfiguration>
	<ProxyGroup name="sources">
	<SourceProxy
		name="SQLiteReader"
		class="vtkSQLiteReader">

		<!--named Aaa as currently ParaView decides which file is the
			main data file by that file with the name which is alphabetically
			first -->
			

		
		<StringVectorProperty
			name="AaaFileName"
			command="SetFileName"
			number_of_elements="1">
			
			<FileListDomain name="files"/>
			<Documentation>
				Reads in points from SQLite3 database
			</Documentation>
		</StringVectorProperty>

		<!--  -->
		<IntVectorProperty
			name="DisplayOnlySelectedData"
			command="SetDisplayOnlySelectedData"
			number_of_elements="1"
			default_values="0">
			<BooleanDomain name="bool" />
			<Documentation>
				
			</Documentation>
		</IntVectorProperty>

		<!--  -->
		<IntVectorProperty
			name="DisplaySelected"
			command="SetDisplaySelected"
			number_of_elements="1"
			default_values="1">
			<BooleanDomain name="bool" />
			<Documentation>
				
			</Documentation>
		</IntVectorProperty>

		<!--  -->
		<IntVectorProperty
			name="DisplaySelectedSnapshot"
			command="SetDisplaySelectedSnapshot"
			number_of_elements="1"
			default_values="0">
			<BooleanDomain name="bool" />
			<Documentation>
				
			</Documentation>
		</IntVectorProperty>		

		<!-- Sets the snapshot to highlight -->
		<IntVectorProperty
			name="DisplaySelectedSnapshotNr"
			command="SetDisplaySelectedSnapshotNr"
			number_of_elements="1"
			default_values="1">
			
			<IntRangeDomain
				name="range"
				min="0"
				max="100"/>
				
			<Documentation>
				Sets the snapshot id to highlight
			</Documentation>
		</IntVectorProperty>


		<!--  -->
		<IntVectorProperty
			name="DisplaySelectedTrack"
			command="SetDisplaySelectedTrack"
			number_of_elements="1"
			default_values="0">
			<BooleanDomain name="bool" />
			<Documentation>
				
			</Documentation>
		</IntVectorProperty>		
		
		<!-- Sets the track to highlight -->
		<IntVectorProperty
			name="DisplaySelectedTrackNr"
			command="SetDisplaySelectedTrackNr"
			number_of_elements="1"
			default_values="1">
			
			<IntRangeDomain
				name="range"
				min="0"
				max="1000"/>
				
			<Documentation>
				Sets the snapshot id to highlight
			</Documentation>
		</IntVectorProperty>

		<!--  -->
		<IntVectorProperty
			name="DisplayCalculated"
			command="SetDisplayCalculated"
			number_of_elements="1"
			default_values="0">
			<BooleanDomain name="bool" />
			<Documentation>
				
			</Documentation>
		</IntVectorProperty>
		
		<!-- -->
		<DoubleVectorProperty
			name="CalculationImpactParameter"
			command="SetCalculationImpactParameter"
			number_of_elements="1"
			default_values="0.00000001">
			<Documentation>
				
			</Documentation>
		</DoubleVectorProperty>

		<!--  -->
		<IntVectorProperty
			name="DisplayEstimateTolerance"
			command="SetDisplayEstimateTolerance"
			number_of_elements="1"
			default_values="0">
			<BooleanDomain name="bool" />
			<Documentation>
				
			</Documentation>
		</IntVectorProperty>
		<!--
		<StringVectorProperty
			name="SelectInputArray" 
			command="SetInputArrayToProcess" 
			number_of_elements="5" 
			element_types="0 0 0 0 2" 
			animateable="0"> 
			<ArrayListDomain name="array_list" 
			   attribute_type="Scalars">
				<RequiredProperties>
					<Property name="Input" function="Input"/>
				</RequiredProperties>
			</ArrayListDomain>
			<FieldDataDomain name="field_list">
				<RequiredProperties>
					<Property name="Input" function="Input"/>
				</RequiredProperties>
			</FieldDataDomain>
			<Documentation>
				This property indicates in which array the global ids are stored, required if running in parallel, otherwise not used. These global ids can be generated by D3, the Tipsy Reader, or read in as an additional attribute.
			</Documentation>
		</StringVectorProperty>
		-->
		
	</SourceProxy>
	</ProxyGroup>
</ServerManagerConfiguration>
